    "pack:ios": "node bin/packIOS",
    "pack:ppk": "node bin/packPPK",
    "pack:android": "node bin/packAndroid",
    "updateYS": "node bin/updateYS",
    "reinstall": "rm yarn.lock && rm -rf node_modules && npm run yarnInstall",
